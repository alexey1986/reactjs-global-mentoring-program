{
    "name": "reactjs-global-mentoring-program",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build:prod": "webpack --env=prod",
        "build:dev": "webpack --env=dev",
        "start": "webpack-dev-server --env=dev --open --hot",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:updateSnapshots": "jest --updateSnapshot",
        "cypress:open": "cypress open",
        "next:dev": "next dev",
        "next:start": "next start",
        "next:build": "next build"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./src/__mocks__/fileMock.js",
            "\\.(css|less)$": "identity-obj-proxy",
            "^@root(.*)$": "<rootDir>/src$1",
            "^@src(.*)$": "<rootDir>/src$1",
            "^@components(.*)$": "<rootDir>/src/components$1",
            "^@actions(.*)$": "<rootDir>/src/actions$1",
            "^@actionTypes(.*)$": "<rootDir>/src/actionTypes$1",
            "^@reducers(.*)$": "<rootDir>/src/reducers$1"
        },
        "modulePaths": [
            "src"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "modulePathIgnorePatterns": [
            "cypress"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alexey1986/reactjs-global-mentoring-program.git"
    },
    "keywords": [
        "react",
        "nextjs",
        "react-testing-library"
    ],
    "author": "Aliaksei Paleshchuk",
    "license": "",
    "bugs": {
        "url": "https://github.com/alexey1986/reactjs-global-mentoring-program/issues"
    },
    "homepage": "https://github.com/alexey1986/reactjs-global-mentoring-program#readme",
    "dependencies": {
        "axios": "^0.21.4",
        "bootstrap": "^4.6.1",
        "cypress": "^8.7.0",
        "formik": "^2.2.9",
        "next": "^12.0.4",
        "next-redux-wrapper": "^7.0.5",
        "react": "^16.14.0",
        "react-bootstrap": "^1.6.4",
        "react-bootstrap-formik": "^1.7.2",
        "react-dom": "^16.14.0",
        "react-redux": "^7.2.6",
        "redux": "^4.1.2",
        "redux-devtools-extension": "^2.13.9",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-react": "^7.16.0",
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@wdio/cli": "^7.16.9",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.3",
        "css-loader": "^2.1.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^3.0.1",
        "html-react-parser": "^1.4.0",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "mini-css-extract-plugin": "^1.6.2",
        "optimize-css-assets-webpack-plugin": "^5.0.8",
        "prop-types": "^15.7.1",
        "react-test-renderer": "^16.14.0",
        "redux-devtools": "^3.7.0",
        "redux-mock-store": "^1.5.4",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.11.3",
        "webpack-merge": "^5.8.0"
    }
}
